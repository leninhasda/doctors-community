<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
 
<struts>

    <constant name="struts.devMode" value="false"/>

	<package name="default"   extends="struts-default">

        <action name="test" class="com.jonak.controller.UserController" method="test">
            <result name="success">/home.jsp</result>
        </action>

        <action name="register">
            <result>/register.jsp</result>
        </action>

        <action name="registerUser" class="com.jonak.controller.UserController" method="register">
            <result name="success">/home.jsp</result>
        </action>

        <action name="login" class="com.jonak.controller.UserController" method="login">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="forgetpassword">
            <result>/forgetpassword.jsp</result>
        </action>

        <action name="getmail" class="com.jonak.controller.UserController" method="forgetPassword">
            <result name="success">/check_email.jsp</result>
        </action>

        <action name="resetpassword" class="com.jonak.controller.UserController" method="findUser">
            <result name="success">/reset_password.jsp</result>
        </action>

        <action name="setnewpassword" class="com.jonak.controller.UserController" method="setPassword">
            <result name="success">/index.jsp</result>
        </action>

        <action name="nopasswordreset" class="com.jonak.controller.UserController" method="noReset">
            <result name="success">/index.jsp</result>
        </action>

        <action name="add_profile">
            <result>/addprofile.jsp</result>
        </action>

        <action name="create_profile" class="com.jonak.controller.ExperienceController" method="create">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="show_experience" class="com.jonak.controller.ExperienceController" method="viewExperience">
            <result name="success">/view_experience.jsp</result>
        </action>

        <action name="update_experience" class="com.jonak.controller.ExperienceController" method="setContentID">
            <result name="success">/update_experience.jsp</result>
        </action>

        <action name="update_exp" class="com.jonak.controller.ExperienceController" method="update">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="delete_experience" class="com.jonak.controller.ExperienceController" method="delete">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="add_education">
            <result>/addeducation.jsp</result>
        </action>

        <action name="add_edu" class="com.jonak.controller.EducationController" method="add">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="show_education" class="com.jonak.controller.EducationController" method="viewEducation">
            <result name="success">/view_education.jsp</result>
        </action>

        <action name="update_education" class="com.jonak.controller.EducationController" method="setContentID">
            <result name="success">/update_education.jsp</result>
        </action>

        <action name="update_edu" class="com.jonak.controller.EducationController" method="update">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="delete_education" class="com.jonak.controller.EducationController" method="delete">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="add_chamber">
            <result>/addchamber.jsp</result>
        </action>

        <action name="add_chamber_detail" class="com.jonak.controller.ChamberController" method="add">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="show_chamber" class="com.jonak.controller.ChamberController" method="viewChamber">
            <result name="success">/view_chamber.jsp</result>
        </action>

        <action name="update_chamber" class="com.jonak.controller.ChamberController" method="setContentID">
            <result name="success">/update_chamber.jsp</result>
        </action>

        <action name="update_chamber_detail" class="com.jonak.controller.ChamberController" method="update">
            <result name="success">/welcome_user.jsp</result>
        </action>

        <action name="delete_chamber" class="com.jonak.controller.ChamberController" method="delete">
            <result name="success">/welcome_user.jsp</result>
        </action>
	</package>
	
</struts>